{
  "name": "@zestia/ember-wrap-urls",
  "version": "1.0.24",
  "description": "Automatically turns URLs into Hyperlinks for user-generated-content",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "yarn lint:js && yarn lint:hbs && yarn lint:css",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:css": "stylelint --allow-empty-input '**/*.{css,scss}'",
    "start": "ember serve",
    "test": "ember test && yarn lint",
    "test:all": "ember try:each"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-wrap-urls.git"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "author": "Zestia",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "@zestia/ember-template-lint-plugin": "2.0.1",
    "@zestia/eslint-config": "2.0.14",
    "@zestia/prettier-config": "0.0.3",
    "@zestia/stylelint-config": "2.0.11",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-github-pages": "0.2.1",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-release-tag": "0.1.5",
    "ember-cli-sri": "2.1.1",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-cli": "3.10.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.4.1",
    "ember-resolver": "5.1.3",
    "ember-source-channel-url": "2.0.1",
    "ember-source": "3.10.0",
    "ember-template-lint": "1.1.0",
    "ember-try": "1.2.1",
    "eslint": "5.16.0",
    "loader.js": "4.7.0",
    "prettier": "1.17.1",
    "qunit-dom": "0.8.5",
    "sass": "1.20.3",
    "stylelint": "10.0.1"
  },
  "dependencies": {
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-angle-bracket-invocation-polyfill": "^1.3.1",
    "ember-named-arguments-polyfill": "^1.0.0"
  },
  "keywords": [
    "ember-addon",
    "url",
    "hyperlink",
    "pattern",
    "ugc",
    "user generated content"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-wrap-urls/issues"
  },
  "homepage": "https://github.com/zestia/ember-wrap-urls",
  "configPath": "tests/dummy/config"
}
