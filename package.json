{
  "name": "@zestia/ember-wrap-urls",
  "version": "2.0.1",
  "description": "Automatically turns URLs into Hyperlinks for user-generated-content",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "yarn lint:js && yarn lint:hbs && yarn lint:css",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:css": "stylelint --allow-empty-input '**/*.{css,scss}'",
    "prettier": "prettier '**/*.{js,json,css,scss,html,md}' --write",
    "start": "ember serve",
    "test": "ember test && yarn lint",
    "test:all": "ember try:each"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-wrap-urls.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "author": "Zestia",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "1.0.0",
    "@zestia/ember-template-lint-plugin": "2.0.4",
    "@zestia/eslint-config": "2.0.28",
    "@zestia/prettier-config": "0.0.5",
    "@zestia/stylelint-config": "2.0.22",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.13.1",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-htmlbars-inline-precompile": "3.0.1",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-release-tag": "0.1.5",
    "ember-cli-sri": "2.1.1",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.5.1",
    "ember-resolver": "5.3.0",
    "ember-source": "3.13.2",
    "ember-source-channel-url": "2.0.1",
    "ember-template-lint": "1.5.3",
    "ember-try": "1.2.1",
    "eslint": "6.5.1",
    "loader.js": "4.7.0",
    "prettier": "1.18.2",
    "qunit-dom": "0.9.0",
    "sass": "1.23.0",
    "stylelint": "11.0.0"
  },
  "dependencies": {
    "ember-angle-bracket-invocation-polyfill": "^2.0.2",
    "ember-cli-babel": "^7.12.0",
    "ember-cli-htmlbars": "^4.0.5",
    "ember-named-arguments-polyfill": "^1.0.0"
  },
  "keywords": [
    "ember-addon",
    "url",
    "hyperlink",
    "pattern",
    "ugc",
    "user generated content"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-wrap-urls/issues"
  },
  "homepage": "https://github.com/zestia/ember-wrap-urls",
  "configPath": "tests/dummy/config"
}
